{
    "name": "diabl0/config-knp-menu-bundle",
    "type": "symfony-bundle",
    "description": "Configure your knp menus with yaml files accross your project bundles",
    "keywords": ["Knp", "UI", "User Interface", "Menu", "Navigation", "Symfony"],
    "homepage": "https://github.com/diabl0/ConfigKnpMenuBundle",
    "license": "MIT",
    "require": {
        "knplabs/knp-menu-bundle": "^2.0",
        "symfony/event-dispatcher": "^4.0 | ^5.0",
        "symfony/yaml": "^4.0 | ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "squizlabs/php_codesniffer": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "CKMB\\Bundle\\ConfigKnpMenuBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CKMB\\Bundle\\ConfigKnpMenuBundle\\Tests\\": "Tests/"
        }
    }
}
